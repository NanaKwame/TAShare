<!DOCTYPE html>
<html>
<head>
  <title>TAShare</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="center">
    <% flash.each do |key, value| %>
      <div class="alert alert-<%= key %>"><%= value %></div>
    <% end %>
  </div>
  <% if current_user != nil%>
  <%= link_to('Logout', destroy_user_session_path, :method => :delete) %> 
  <%end%>
<%= yield %>

</body>
</html>
